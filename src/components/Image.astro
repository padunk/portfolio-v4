---
import { urlFor } from '@/utils'
import { Image as ImageAstro } from 'astro:assets'
type Asset = Parameters<typeof urlFor>[0]

export interface Props {
  asset: Asset
  alt: string
}

const { asset, alt } = Astro.props
const image = urlFor(asset).url()
---

<ImageAstro
  src={image}
  alt={alt}
  width={650}
  height={400}
  quality='mid'
  format='webp'
/>
